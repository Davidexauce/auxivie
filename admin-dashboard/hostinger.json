{
  "name": "auxivie-admin-dashboard",
  "version": "1.0.0",
  "description": "Dashboard d'administration Auxivie",
  "type": "nextjs",
  "build": {
    "command": "npm install && npm run build",
    "output": ".next"
  },
  "start": {
    "command": "npm start",
    "port": 3000
  },
  "env": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_API_URL": "https://api.auxivie.org"
  }
}
